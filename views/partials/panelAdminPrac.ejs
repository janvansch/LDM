<h2>Practise Administration</h2>
<!--
=== Display Practise list ===
-->
<hr/>
<h3>Practise List</h3>
<div id="tablePos10"></div>
<hr/>
<input type="button" value="Add User" onclick="javascript:toggleAddUser();"/>
<input type="button" value="Delete User" onclick="javascript:deleteUser();"/>
<input type="button" value="Update User" onclick="javascript:updateUser();"/>
<!--
=== Add Practise ===
-->
<div id="addPractise" style="display:none">
  <% include ./addPractise %>
</div>
<!--
=== Delete User ===
-->
<div id="deletePractise" style="display:none">
  <fieldset>
    <legend>Delete User</legend>
    <label>
      Firstname: <input type="text" name="userdelete" size="15"/>
    </label>
    <input type="button" value="Delete User" onclick="deleteUser()"/>
  </fieldset>
</div>
<!--
=== End of Delete User ===
-->
<!--
=== Update User ===
-->
<div id="updatePractise" style="display:none">
  <fieldset>
    <legend>Update User</legend>
    <label>
      Firstname: <input type="text" name="userupdate" size="15"/>
    </label>
    <label>
      Surname: <input type="text" name="userupdate" size="20"/>
    </label>
    <input type="button" value="Update User" onclick="updateUser()"/>
  </fieldset>
</div>
<!--
=== End of Update User ===
-->
